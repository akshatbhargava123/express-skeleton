{
  "name": "express-skeleton",
  "version": "1.0.0",
  "description": "A starter template for Express JS apps, with sane defaults (uses EJS, Mongoose, Gulp, Eslint, etc) ",
  "main": "bin/www",
  "author": "Sayan \"Riju\" Chakrabarti <s26c.sayan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/www",
    "lint": "npm run lint:quiet",
    "lint:quiet": "eslint --quiet ./**/*.js",
    "lint:all": "eslint ./**/*.js",
    "lint:fix": "eslint ./**/*.js --quiet --fix",
    "prepush": "npm run lint:quiet"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-mailer": "^0.3.1",
    "express-session": "^1.14.1",
    "helmet": "^3.4.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "mongoose": "^4.5.10",
    "morgan": "^1.9.0",
    "multer": "^1.2.0",
    "randomstring": "^1.1.5",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.6",
    "tinyurl": "^1.1.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.1.0",
    "gulp-notify": "^2.2.0",
    "husky": "^0.14.3"
  },
  "eslintIgnore": ["public/", "bin/"]
}
